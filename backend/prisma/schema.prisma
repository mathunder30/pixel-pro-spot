generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id       Int    @id @default(autoincrement())
  nome     String @db.VarChar(300)
  email    String @unique @db.VarChar(200)
  mensagem String @db.VarChar(250)
}
